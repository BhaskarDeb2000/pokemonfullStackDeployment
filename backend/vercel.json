{
  "version": 2,
  "builds": [
    { "src": "routes/HomeRoute.js", "use": "@vercel/node" },
    { "src": "routes/AbilitiesRoute.js", "use": "@vercel/node" },
    { "src": "routes/ImageRoute.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/pokemon", "dest": "/routes/HomeRoute.js" },
    { "src": "/pokemon/abilities", "dest": "/routes/AbilitiesRoute.js" },
    { "src": "/pokemon/image", "dest": "/routes/ImageRoute.js" }
  ]
}
